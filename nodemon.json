{
    "restartable": "rs",
    "verbose": false,
    "env": {
        "NODE_ENV": "development",
        "PORT": "3000"
    },
    "execMap": {
        "": "node --inspect",
        "js": "node --inspect"
    },
    "watch": [
        "src/",
        "index.html"
    ],
    "ignore": [
        ".git",
        ".idea",
        "node_modules"
    ],
    "ext": "js vue"
}