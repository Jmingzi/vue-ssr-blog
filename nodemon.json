{
  "restartable": "rs",
  "verbose": false,
  "env": {
    "NODE_ENV": "production",
    "PORT": "3000"
  },
  "execMap": {
    "": "node --inspect",
    "js": "node --inspect"
  },
  "watch": [
    "./"
  ],
  "ignore": [
    ".git",
    ".idea",
    "node_modules",
    "dist",
    "log",
    "README.md"
  ],
  "ext": "js vue"
}
